{
  "name": "llmonitor",
  "version": "0.5.0",
  "description": "llmonitor is an open-source monitoring and analytics platform for AI apps.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./openai": {
      "types": "./dist/openai.d.ts",
      "import": "./dist/openai.mjs",
      "require": "./dist/openai.js",
      "default": "./dist/openai.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "openai": [
        "./dist/openai.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && tsup --entry src/index.ts --entry src/openai.ts --format esm,cjs --dts && rm dist/*.d.cts"
  },
  "author": "llmonitor",
  "license": "Apache-2.0",
  "dependencies": {
    "unctx": "^2.3.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@types/node": "^20.5.1",
    "dotenv": "^16.3.1",
    "langchain": "^0.0.131",
    "openai": "^4.3.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "openai": "^4.3.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  }
}
